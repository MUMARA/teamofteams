        }
      }
    },
    "groups": {
      "$groupid": {
        ".validate": "newData.hasChildren(['title', 'desc', 'timestamp', 'members-count', 'subgroups-count', 'address', 'phone', 'timeZone', 'members-checked-in', 'logo-image', 'privacy'])",
        "title": {
